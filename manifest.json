{
	"name": "idn-atk-ext",
	"version": "1.0",
	"manifest_version": 2,
	"description": "An extension to prevent IDN Homograph attacks",  
	"background": { 
		"scripts": ["myscript.js"] 
		},
	"permissions": [
		"tabs", "http://*/*"
  	],
	"chrome_url_overrides" : {
		"newtab": "newtabOverride.html"
  	},
    "content_scripts": [
    	{
		 "matches": ["http://*/*"],
		 "js": ["inject.js"],
		 "run_at": "document_start"
    	}
  	]
}
